{
  "expo": {
    "entryPoint": "./src/index.tsx",
    "name": "Song Contest Rater",
    "slug": "song-contest-rater-app",
    "scheme": "songcontestrater",
    "privacy": "unlisted",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#2a2a2a"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.hyperremix.songcontestrater",
      "buildNumber": "1.0.0",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "This app requires the permission to use the photo library for using already existing photos as avatars."
      }
    },
    "android": {
      "package": "com.hyperremix.songcontestrater",
      "versionCode": 10000,
      "permissions": [
        "READ_EXTERNAL_STORAGE",
        "ACCESS_NETWORK_STATE",
        "ACCESS_WIFI_STATE"
      ],
      "softwareKeyboardLayoutMode": "pan"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
