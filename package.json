{
  "name": "song-contest-rater-app",
  "version": "0.1.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "checkTs": "tsc --noEmit",
    "eslint": "eslint --ext js,ts,tsx",
    "generate": "cross-env TS_NODE_PROJECT='./internals/ts-node.tsconfig.json' plop --plopfile internals/generators/plopfile.ts",
    "prettier:check": "prettier -c 'src/**/*[.ts|.tsx]'",
    "prettier:fix": "prettier -w 'src/**/*[.ts|.tsx|.json|.js]'",
    "syncversion": "node ./scripts/syncVersionToAppJson.js && yarn prettier -w './app.json'",
    "patch": "yarn version --new-version patch",
    "minor": "yarn version --new-version minor",
    "major": "yarn version --new-version major",
    "version": "yarn syncversion && git add app.json",
    "prepare": "husky install"
  },
  "dependencies": {
    "@eva-design/material": "^2.1.1",
    "@react-native-async-storage/async-storage": "~1.15.0",
    "@react-native-community/masked-view": "0.1.10",
    "@react-navigation/bottom-tabs": "^5.11.8",
    "@react-navigation/drawer": "^5.12.4",
    "@react-navigation/native": "^5.9.3",
    "@react-navigation/stack": "^5.14.3",
    "@reduxjs/toolkit": "^1.5.0",
    "@ui-kitten/components": "^5.1.1",
    "@ui-kitten/eva-icons": "^5.0.0",
    "expo": "^44.0.0",
    "expo-app-loading": "~1.3.0",
    "expo-device": "~4.1.0",
    "expo-image-picker": "~12.0.1",
    "expo-localization": "~12.0.0",
    "expo-status-bar": "~1.2.0",
    "i18next": "^20.1.0",
    "moment": "^2.29.1",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-i18next": "^11.8.11",
    "react-native": "0.64.3",
    "react-native-gesture-handler": "~2.1.0",
    "react-native-locale-detector": "^1.0.1",
    "react-native-pager-view": "5.4.9",
    "react-native-reanimated": "~2.3.1",
    "react-native-safe-area-context": "3.3.2",
    "react-native-screens": "~3.10.1",
    "react-native-svg": "12.1.1",
    "react-native-tab-view": "^3.0.1",
    "react-native-web": "0.17.1",
    "react-redux": "^7.2.2",
    "react-router-native": "^5.2.0",
    "redux-injectors": "^1.3.0",
    "redux-persist": "^6.0.0",
    "redux-persist-expo-filesystem": "^2.0.0",
    "redux-saga": "^1.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@types/react": "~17.0.21",
    "@types/react-native": "~0.64.12",
    "@types/react-redux": "^7.1.16",
    "@types/react-router-native": "^5.1.0",
    "@types/redux-persist": "^4.3.1",
    "@types/rimraf": "^3.0.0",
    "@types/shelljs": "^0.8.8",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "chalk": "4.1.2",
    "cross-env": "7.0.3",
    "dotenv": "^14.3.2",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-config-react-app": "^7.0.0",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "expo-cli": "^5.0.3",
    "husky": "^7.0.1",
    "inquirer": "7.3.3",
    "inquirer-directory": "2.2.0",
    "lint-staged": "12.1.7",
    "node-plop": "0.26.2",
    "plop": "2.7.4",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2",
    "shelljs": "^0.8.4",
    "ts-node": "^9.1.1",
    "typescript": "~4.4.4"
  },
  "private": true,
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": "eslint --fix",
    "**/*": "prettier --write --ignore-unknown"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run checkTs && lint-staged"
    }
  }
}
